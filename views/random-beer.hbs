{{#each beersRandomFromApi}}
<div >
    <img src={{this.image_url}} />
    <div >
        <p>
            <h3>{{this.name}}</h3>
            <p>{{this.tagline}}</p>
        </p>
        <p>{{this.description}}</p>
        <p>{{this.brewers_tips}}</p>
        <p>{{this.id}}</p>
        <ul>
            <li>{{this.food_pairing}}</li>
        </ul>
    </div>
</div>
{{/each}}